pipeline{
    agent any
    stages{
        stage('Test'){
            steps{
                echo "Testing the code - phase-1"
                echo "Testing the code - phase-2"
            }
        }
        stage('Build'){
            steps{
                echo "Building the code"
            }
        }
        stage('Release'){
            steps{
                echo "Releasing the code"
            }
        }
        stage('Deploy'){
            steps{
                echo "Deploying the code"
            }
        }
    }
}